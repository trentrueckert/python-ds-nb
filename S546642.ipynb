    "# Number of rows\n",
    "rows = 4\n",
    "\n",
    "# Loop to make each row\n",
    "for i in range(1, rows+1):\n",
    "    # Stars\n",
    "    print('*' * i, end = '')\n",
    "    # Spaces\n",
    "    print(' ' * (2 * (rows-i)), end = '')\n",
    "    # Stars\n",
    "    print('*' * i)"
   ]
